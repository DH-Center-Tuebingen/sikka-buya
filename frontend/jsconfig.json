{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "~/*": [
        "./*"
      ],
      "@/*": [
        "./*"
      ],
      "~~/*": [
        "./*"
      ],
      "@@/*": [
        "./*"
      ]
    }
  },
  "vueOptions": {
    "target": 2
  },
  "vueCompilerOptions": {
    "target": 2,
  },
  "extensions": [
    ".vue"
  ],
  "include": [
    "node_modules/vue-material-design-icons/*"
  ],
  "exclude": [
    "node_modules",
    ".nuxt",
    "dist"
  ]
}